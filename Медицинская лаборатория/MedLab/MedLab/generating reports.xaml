<Window x:Class="MedLab.generating_reports"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MedLab"
        mc:Ignorable="d"
        Title="Сгенерировать отчеты" Height="450" Width="800">
    <Grid>
        <Rectangle HorizontalAlignment="Center" Height="36" Stroke="ForestGreen" VerticalAlignment="Top" Width="800" Fill="ForestGreen"/>
        <DataGrid x:Name="data2"  Margin="34,51,34,51"/>
        <Label Content="Отчеты" HorizontalAlignment="Left" Margin="93,1,0,0" VerticalAlignment="Top" FontFamily="Comic Sans MS" FontWeight="Bold" FontSize="18"/>
        <Button Content="Распечатать" HorizontalAlignment="Center" Margin="0,388,0,0" VerticalAlignment="Top" Height="36" Width="246" FontWeight="Bold" FontSize="20" FontFamily="Comic Sans MS" Click="Button_Click"/>

    </Grid>
</Window>
